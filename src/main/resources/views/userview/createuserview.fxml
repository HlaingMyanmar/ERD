<AnchorPane prefHeight="250.0" prefWidth="331.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.erd.useroptions.controllers.UserControllers">
   <children>
      <VBox layoutX="14.0" layoutY="13.0" prefHeight="154.0" prefWidth="304.0" styleClass="pane-full-shadow" stylesheets="@../../css/pane.css">
