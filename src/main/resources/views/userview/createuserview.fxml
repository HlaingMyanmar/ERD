            <TextField fx:id="usertxt" promptText="အသုံးပြုသူအမည်ထည့်ပါ">
               <font>
                  <Font name="Pyidaungsu" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="passtxt" promptText="စကားဝှက်">
               <font>
                  <Font name="Pyidaungsu" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </PasswordField>
            <HBox prefHeight="43.0" prefWidth="304.0">
               <children>
                  <JFXCheckBox fx:id="enablecheckbox" text="ဖွင့်မည်။">
                     <font>
                        <Font name="Pyidaungsu" size="12.0" />
