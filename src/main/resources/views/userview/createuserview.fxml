                        <Font name="Pyidaungsu" size="12.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin>
                  </JFXCheckBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Button fx:id="insertbtn" layoutX="13.0" layoutY="183.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="304.0" stylesheets="@../../css/btn2.css" text="အသစ်ထည့်မည်။">
         <font>
            <Font name="Pyidaungsu Bold" size="12.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
         <graphic>
            <FontAwesomeIconView glyphName="USER" glyphStyle="-fx-fill:white" size="24" />
         </graphic>
      </Button>
   </children>
